Program
{
    integer inc(integer x)
    {
        return x + x;
    }

    integer main()
    {
        integer x = 12;
        inc(x+x);
        if (x < 12) then
        {
            x = x + x;
        } 
        else {
            x = 2;
        }
        while (x < 23)
        {
            x = x + x;
        }
        return 1;
    }
}

