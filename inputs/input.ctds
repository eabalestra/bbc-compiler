Program {
    
    integer inc(integer x) {
        return x + 1;
    }

    integer get_integer() extern;
    void printInteger(integer x) extern;
    
    void main() {
        integer y;
        y = inc(get_integer());
        if (y == 1) then {
            printInteger(1);
        } else {
            printInteger(0);
        }
        return;
    }
}