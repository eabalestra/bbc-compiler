Program
{
    integer x = 1;

    integer inc(integer x)
    {
        return x + x;
    }
    
    integer get_integer() extern;

    void printInteger(integer i) extern ;

    void main()
    {
        integer y = 0;
        y = get_integer();
        if (y == 1) then
        {
            printInteger(y);
        }
        else
        {
            return printInteger(inc(y));
        }
    }
}